@Data_API

metamodel Persons {
	PersonRegister {
		<>-persons(0..*)->Person
	}
	
	Person {
		.name : EString
		.birthday : EDate
		
		-personsInverse(0..1)->PersonRegister
	}
	
	Male : Person
	Female : Person
}

abstract pattern PersonPattern {
	register:PersonRegister {
		-persons->person
	}
	
	person:Person
}

pattern MalePattern -> PersonPattern {
	person:Male
}

pattern FemalePattern -> PersonPattern {
	person:Female
}
